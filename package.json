{
  "name": "vscode-create-spec",
  "displayName": "vscode-create-spec",
  "description": "Easy create a spec file for the opened filed",
  "version": "0.0.2",
  "publisher": "raulmelo",
  "engines": {
    "vscode": "^1.31.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raulfdm/vscode-create-spec"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createSpec"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createSpec",
        "title": "Create Spec File"
      }
    ]
  },
  "scripts": {
    "prebuild": "rimraf out",
    "build": "tsc -p ./ --skipLibCheck",
    "watch": "tsc -watch -p ./",
    "predeploy": "vsce package",
    "deploy": "vsce publish",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run build && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "rimraf": "^2.6.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vsce": "^1.57.1",
    "vscode": "^1.1.28"
  }
}
